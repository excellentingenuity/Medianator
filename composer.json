{
  "name": "eig/medianator",
  "description": "A PHP Media Handler Package",
  "license": "MIT",
  "keywords": ["Media Handeling", "images", "audio", "video", "media"],
  "type": "library",
  "homepage": "https://github.com/excellentingenuity/medianator",
  "authors": [
    {
      "name": "James Johnson",
      "email": "james.johnson@excellentingenuity.com",
      "role": "Developer/Maintainer"
    }
  ],
  "support": {
    "docs": "https://github.com/excellentingenuity/medianator",
    "issues": "https://github.com/excellentingenuity/medianator/issues",
    "wiki": "https://github.com/excellentingenuity/medianator/wiki"
  },
  "require": {
    "php": ">=5.6",
    "league/flysystem": "^1.0"

  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "mockery/mockery": "^0.9.4",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "eig\\Medianator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "eig\\Medianator\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "master": "master-dev"
    }
  }
}
